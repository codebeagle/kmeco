---
import Default from './Default.astro';
import BannerPage from '../components/BannerPage.astro';
import SideBySideStats from '../components/SideBySideStats.astro';
import ContentTextList from '../components/ContentTextList.astro';
import GalleryImages from '../components/GalleryImages.astro';

const { frontmatter } = Astro.props;
---

<Default {frontmatter}>
  <BannerPage componentData={frontmatter.bannerData}></BannerPage>
  <SideBySideStats componentData={frontmatter.sbsStatsData}></SideBySideStats>
  <ContentTextList componentData={frontmatter.textListData}></ContentTextList>
  <GalleryImages componentData={frontmatter.galleryImagesData}></GalleryImages>
</Default>